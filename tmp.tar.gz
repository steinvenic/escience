        			return  this.optional( element ) ||/^[a-zA-Z0-9.!#$%&'*+\/=?^_`{|}~-]+@([a-z0-9A-Z]+(-[a-z0-9A-Z]+)?\.)+[a-zA-Z]+$/.test( value );

        		}

        });
</script>

